{
  "name": "platzi-overflow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -r \"npm run start:server\" \"npm run start:client\"",
    "start:server": "NODE_ENV=development DEBUG=platzi-overflow* nodemon server/index.js --exec babel-node",
    "start:client": "ng serve",
    "start-prod" : "NODE_ENV=production node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "npm run build:client && npm run build:server",
    "build:client": "ng build --configuration=production",
    "build:server": "babel server --out-dir dist/server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/cli": "~6.1.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "angular2-moment": "^1.9.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "core-js": "^2.5.4",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.2",
    "mongoose-unique-validator": "^2.0.2",
    "nodemon": "^1.18.4",
    "rxjs": "^6.0.0",
    "sweet-scroll": "^3.0.1",
    "typescript": "~2.7.2",
    "url-join": "^4.0.0",
    "urljoin": "^0.1.5",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-preset-stage-0": "^6.24.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
